    <quoteString>Y</quoteString>
    <sql>INSERT INTO STEPEXECUTION (stepexecution_id, stepexecution_label, stepexecution_errormessage, stepexecution_status, workflowexecution_id, stepcomposition_id)
VALUES (?, -- channel_id


(select distinct stepname 
 from prov.step
 where channel_id = ? -- channel_id
),


(select log_field
 from prov.step
 where channel_id = ?
 and log_date = (select max(log_date) from prov.step where channel_id = ?)
),

(select  distinct CASE
					WHEN errors = 0
					 	THEN 'end' 
                	when errors &lt;&gt; 0
					 	then 'with errors'
         		  END  status
 from prov.step
 where channel_id = ? -- channel_id
 and log_date = (select max(log_date) from prov.step where channel_id = ?	)

),

(select distinct parent_channel_id
 from prov.logchannel
 where channel_id = ?
),

(select stepcomposition_id
 from stepcomposition s, prov.step st
 where s.stepcomposition_label  = st.stepname
 and st.channel_id = ?
 and st.log_date = (select max(log_date)
					from prov.step st2
					where st.stepname = st2.stepname
					and st2.channel_id = ?)
)

);







</sql>
